			samsung,pins = "gpa0-4";
			samsung,pin-function = <0x0>;/*input*/
			samsung,pin-pud = <1>;/*pull dwon*/
		};

		sys3p3_ldo_en_off: sys3p3_ldo_en_off {
			samsung,pins = "gpa1-3";
			samsung,pin-function = <0x0>;/*input*/
			samsung,pin-pud = <1>;/*pull dwon*/
		};

		hdmi_dcdc_en_off: hdmi_dcdc_en_off {
			samsung,pins = "gpa2-0";
			samsung,pin-function = <0x0>;/*input*/
			samsung,pin-pud = <1>;/*pull dwon*/
