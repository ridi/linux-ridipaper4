			samsung,pin-pud = <1>;/*pull dwon*/
		};

		sys1p8_ldo_en_on: sys1p8_ldo_en_on {
			samsung,pins = "gpa0-4";
			samsung,pin-function = <0x1>;/*outpu*/
			samsung,pin-val = <1>;/*out high*/
		};

		sys3p3_ldo_en_on: sys3p3_ldo_en_on {
			samsung,pins = "gpa1-3";
			samsung,pin-function = <0x1>;/*output*/
			samsung,pin-val = <1>;/*out high*/
		};

		hdmi_dcdc_en_on: hdmi_dcdc_en_on {
			samsung,pins = "gpa2-0";
			samsung,pin-function = <0x1>;/*output*/
			samsung,pin-val = <1>;/*out high*/
		};

		lcd_power_on: lcd_power_on {
			samsung,pins = "gpa2-2";
			samsung,pin-function = <0x1>;/*output*/
			samsung,pin-val = <1>;/*out high*/
		};

		lcd_bl_on: lcd_bl_on {
			samsung,pins = "gpa2-3";
			samsung,pin-function = <0x1>;/*input*/
			samsung,pin-val = <1>;/*out high*/
		};

                lt9611_hpd_en: lt9611-hpd {
		samsung,pins = "gpa2-0";
		samsung,pin-function = <0x1>;
		samsung,pin-val = <1>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <0>;
                };

	        lt9611_irq: lt9611-irq {
