/ {
	aliases {
		uart0 = &serial_0;
		usi0 = &usi_hsi2c_0;
		usi1 = &usi_hsi2c_1;
		usi2 = &usi_hsi2c_2;
		usi3 = &usi_uart;
		usi4 = &usi_spi_0;
		usi5 = &usi_00_cmgp;
		usi6 = &usi_01_cmgp;
		hsi2c0 = &hsi2c_0;
		hsi2c1 = &hsi2c_1;
		hsi2c2 = &hsi2c_2;
		hsi2c3 = &hsi2c_3;
		hsi2c4 = &hsi2c_4;
		spi0 = &spi_0;
		spi1 = &spi_1;
		spi2 = &spi_2;
		uart1 = &serial_1;
		uart2 = &serial_2;
		i2c0 = &i2c_0;
		i2c1 = &i2c_1;
		i2c2 = &i2c_2;
		i2c3 = &i2c_3;
		i2c4 = &i2c_4;
		i2c5 = &i2c_5;
		i2c6 = &i2c_6;
	};

	/* USI_HSI2C_0 */
	usi_hsi2c_0: usi@10021020 {
		compatible = "samsung,exynos-usi-v2";
		reg = <0x0 0x10021020 0x4>;
		/*	usi_v2_mode = "i2c" or "spi" or "uart"	*/
		status = "disabled";
	};

	/* USI_HSI2C_1 */
	usi_hsi2c_1: usi@10021030 {
		compatible = "samsung,exynos-usi-v2";
		reg = <0x0 0x10021030 0x4>;
		/*	usi_v2_mode = "i2c" */
		status = "disabled";
	};

	/* USI_HSI2C_2 */
	usi_hsi2c_2: usi@10021040 {
