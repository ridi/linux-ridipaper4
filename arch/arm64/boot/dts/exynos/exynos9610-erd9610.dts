/dts-v1/;
/plugin/;

#include "exynos9610-erd9610_common.dtsi"

/ {
	compatible = "samsung,exynos9610", "samsung,ERD9610";
	board_id = <0x0>;
	board_rev = <0x0>;

	fragment@model {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			model = "Samsung ERD9610 board based on EXYNOS9610";
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */
